#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../../../_staged-git-files@1.1.2@staged-git-files/bin/cli.js" "$@"
  ret=$?
else 
  node  "$basedir/../../../_staged-git-files@1.1.2@staged-git-files/bin/cli.js" "$@"
  ret=$?
fi
exit $ret
