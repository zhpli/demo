#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/bash" ]; then
  "$basedir/bash"  "$basedir/../../../_run-node@1.0.0@run-node/run-node" "$@"
  ret=$?
else 
  bash  "$basedir/../../../_run-node@1.0.0@run-node/run-node" "$@"
  ret=$?
fi
exit $ret
